{
  "name": "Njenje",
  "version": "0.0.1",
  "description": "Calculate the energy consumption from reading the news.",
  "background_page": "background.html",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Time Tracker",
    "popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": { "128": "images/icon.png" },
  "content_scripts": [
    {
      "matches": ["http://*.guardian.co.uk/*"],
      "js": ["idle.js"]
    }
  ]
}
