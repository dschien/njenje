{
  "name": "Njenje",
  "version": "1",
  "description": "Keeps track of how much energy you spend on the Guardian.",
  "background_page": "background.html",
  "permissions": [
    "tabs",
    "geolocation"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Njenje",
    "popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": { "128": "images/icon.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["idle.js"]
    }
  ]
}
